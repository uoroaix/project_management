<h1> Give me a new question</h1>

<%= form_for @question do |f| %>

	<div class="form-group">
		<%= f.label :title %>
		<%= f.text_field :title, class: "form-control" %>
	</div>
	<div class="form-group">
		<%= f.label :description %>
		<%= f.text_area :description, class: "form-control" %>
	</div>
	<%= f.submit "Save", class: "btn btn-warning" %>

<% end %>